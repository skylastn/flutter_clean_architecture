{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dev SkyKomik",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main_dev.dart",
      "flutterMode": "debug",
      "args": [
        "--flavor",
        "development"
      ],
    },
    {
      "name": "SkyKomik",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main_prod.dart",
      "flutterMode": "debug",
      "args": [
        "--flavor",
        "production"
      ],
    },
    {
      "name": "Web SkyKomik",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "Windows SkyKomik",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "args": [
        "-d",
        "windows"
      ]
    },
  ]
}